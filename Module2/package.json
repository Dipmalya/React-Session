{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.jsx",
  "scripts": {
    "build": "cross-env webpack --config ./webpack.config.js --mode production --fix",
    "dev": "cross-env webpack-dev-server --config ./webpack.config.js --open --mode development --fix",
    "test": "cross-env jest --coverage",
    "test:watch": "cross-env jest --watchAll"
  },
  "author": "Dipmalya Sen",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "browserify": "^16.5.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.8.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^7.0.0",
    "react-hot-loader": "^4.12.20",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "uuid": "^7.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.js",
      "src/*.jsx",
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.node/",
      "/jest/"
    ]
  }
}
